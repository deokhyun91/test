  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.project.domain.UserRespository">
	 	<insert id="save" parameterType="com.web.project.domain.User">
		  insert into
		  	 	tb_member
		  values(
		  		0,
